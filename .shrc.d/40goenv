# vim: set ft=sh:

# goenv
if which goenv >/dev/null; then
  eval "$(goenv init -)"
else
  if test -z "$GOENV_ROOT"; then
    for i in ~/.goenv /usr/local/goenv; do
      if test -x "$i/bin/goenv"; then
        export GOENV_ROOT="$i"
        break
      fi
    done
  fi

  if test -n "$GOENV_ROOT"; then
    PATH="$GOENV_ROOT/bin:$PATH"
    eval "$(goenv init -)"
  fi
fi
